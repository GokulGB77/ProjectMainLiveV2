                    </div>
                </div>
            </div>
            <div class="product-slider-active-1 swiper-container">
                <div class="swiper-wrapper">
                    <% allItems.forEach(product=>{ %>
                        <div class="swiper-slide">
                            <div class="product-wrap" data-aos="fade-up" data-aos-delay="200">

                                <div class="product-img img-zoom mb-25">
                                    <a href="product-details?id=<%=product._id %>">
                                        <img src="/productAssets/<%= product.images[0] %>"
                                            alt="<%= product.productName %>">
                                    </a>

                                    <div class="product-action-wrap">
                                        <button class="product-action-btn-1 add-to-wishlist-btn"
                                            data-product-id="<%= product._id %>" title="Wishlist"><i
                                                class="pe-7s-like"></i></button>
                                        <!-- <button class="product-action-btn-1" title="Quick View" data-bs-toggle="modal"
                                            data-bs-target="#exampleModal">
                                            <i class="pe-7s-look"></i>
                                        </button> -->
                                    </div>
                                    <div class="product-action-2-wrap">
